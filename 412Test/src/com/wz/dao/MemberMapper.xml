<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wz.dao.MemberMapper">

<resultMap type="com.wz.pojo.Member" id="memberMap">
		<id property="id" column="id" />
		<result property="userName" column="u_name" />
		<result property="userTel" column="u_tel" />
	</resultMap>


    
    <insert id="addMember" parameterType="com.wz.pojo.Member" keyProperty="memberId" useGeneratedKeys="true">
	insert into member(name,phone_number,grad_year,major,company,job) VALUES(#{name}, #{phoneNumber}, #{gradYear},#{major},#{company}, #{job})
	</insert>

</mapper>